package question3;
import java.util.ArrayList;

public class ArrayListRunner
{

	public static void main(String[] args)
	{
		ArrayList<String> names = new ArrayList<String>();
		System.out.println(names);

		//Adding elements to the Array List
		names.add("Alice");
		names.add("Bob");
		names.add("Connie");
		names.add("David");
		names.add("Edward");
		names.add("Fran");
		names.add("Gomez");
		names.add("Harry");
		System.out.println(names);
	}

}
